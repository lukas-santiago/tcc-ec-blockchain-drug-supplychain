{
  "name": "tcc-ec-blockchain-drug-supplychain-chaincode",
  "version": "0.0.2",
  "description": "",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "start": "fabric-chaincode-node start",
    "build": "tsc",
    "build:all": "npm run build && npm run zip && node scripts/generate-metadata.mjs",
    "build:metadata": "npm run build && node scripts/generate-metadata.mjs",
    "zip": "npm shrinkwrap && zip -r ./zip/file-$(find ./zip| wc -l).zip ./dist ./package.json ./npm-shrinkwrap.json",
    "tar": "npm shrinkwrap && tar -czvf ./zip/file.tar.gz ./dist ./package.json ./npm-shrinkwrap.json",
    "metadata": "node_modules/.bin/fabric-chaincode-node metadata generate",
    "debug": "node_modules/.bin/fabric-chaincode-node metadata generate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.2.1",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "fabric-contract-api": "^2.5.2",
    "fabric-shim": "^2.5.2",
    "json-stringify-deterministic": "^1.0.8",
    "sort-keys-recursive": "^2.1.8",
    "uuid": "^9.0.0"
  }
}
